{
  "name": "cozey-uat-seeder",
  "version": "0.1.0",
  "private": true,
  "description": "Seeder for Shopify staging orders and WMS staging entities.",
  "engines": {
    "node": "20.13.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint \"{src,prisma}/**/*.ts\"",
    "lint:fix": "eslint \"{src,prisma}/**/*.ts\" --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prisma:generate": "prisma generate",
    "seed": "tsx src/cli.ts",
    "generate-config": "tsx src/generateConfig.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/eslint-plugin": "^1.4.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "globals": "^17.0.0",
    "prettier": "^3.4.2",
    "prisma": "^6.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.700.0",
    "@aws-sdk/credential-providers": "^3.700.0",
    "@inquirer/search": "^4.1.0",
    "@prisma/client": "^6.19.2",
    "@shopify/admin-api-client": "^1.0.0",
    "@types/p-limit": "^2.1.0",
    "csv-parse": "^5.5.0",
    "dotenv": "^16.4.0",
    "inquirer": "^10.2.0",
    "p-limit": "^7.2.0",
    "uuid": "^13.0.0",
    "zod": "^3.23.0"
  }
}
